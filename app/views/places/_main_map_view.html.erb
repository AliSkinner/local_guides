<div id="map-canvas"></div>

<button id="reveal-place-form">create new place</button>

<div id="new-form">
  <p>Title: <input size="20" type="text" id="new-title" name="lat" >
  Image: <input type="file" id="new-image"></p>

  <p>Description:</p>
  <textarea name="description" id="new-description" cols="30" rows="5"></textarea>
  <p class="hidden">Latitude: <input size="20" type="text" id="new-lat" name="lat" ></p>
  <p class="hidden">Longitude: <input size="20" type="text" id="new-long" name="lng" ></p>
  <p class="hidden" id="user-id"><%= current_user.id %></p>
  <p><button id="submit-place-form">submit place</button></p>
</div>
